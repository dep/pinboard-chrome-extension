{
  "name": "Pinboard style",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://martineau.tv",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "contentSettings",
    "https://pinboard.in/*",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://pinboard.in/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://pinboard.in/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}